{
  "name": "resumeai-smart-cv-analysis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && node -e \"console.log('Open dist/stats.html generated by visualizer if needed')\"",
    "build:optimized": "node scripts/build-optimized.js",
    "preview": "vite preview",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --collect.numberOfRuns=3 --assert",
    "lighthouse:mobile": "lhci autorun --collect.settings.preset=perf --collect.settings.formFactor=mobile",
    "lighthouse:mobile:3g": "lhci autorun --collect.settings.preset=perf --collect.settings.formFactor=mobile --collect.settings.throttling.rttMs=150 --collect.settings.throttling.throughputKbps=1638.4",
    "lighthouse:mobile:4g": "lhci autorun --collect.settings.preset=perf --collect.settings.formFactor=mobile --collect.settings.throttling.rttMs=40 --collect.settings.throttling.throughputKbps=10240",
    "lighthouse:all": "npm run lighthouse && npm run lighthouse:mobile:3g && npm run lighthouse:mobile:4g",
    "perf:monitor": "node -e \"console.log('Performance monitoring active')\"",
    "perf:budget": "node scripts/performance-budget.js",
    "perf:check": "npm run build:optimized && npm run perf:budget",
    "perf:mobile": "node scripts/mobile-performance-test.js",
    "perf:mobile:3g": "TEST_NETWORK=3g-fast node scripts/mobile-performance-test.js",
    "perf:mobile:4g": "TEST_NETWORK=4g node scripts/mobile-performance-test.js",
    "test:perf": "npm run perf:check",
    "test:perf:mobile": "npm run perf:mobile",
    "check": "npm run build && npm run perf:budget",
    "monitor:start": "npm run dev & npm run perf:monitor",
    "ci:perf": "npm run build:optimized && npm run lighthouse:ci && npm run perf:budget",
    "vercel-build": "npm run build",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "vercel:dev": "vercel dev",
    "vercel:logs": "vercel logs",
    "setup:vercel": "node scripts/setup-vercel.js",
    "setup:stripe-webhook": "node scripts/setup-stripe-webhook.js",
    "setup:vercel-env": "node scripts/setup-vercel-env.js",
    "setup:production": "npm run setup:stripe-webhook && npm run setup:vercel-env",
    "env:status": "node scripts/switch-env-mode.js --status",
    "env:test": "node scripts/switch-env-mode.js --mode test",
    "env:production": "node scripts/switch-env-mode.js --mode production",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:test": "node scripts/deploy-production.js --test",
    "stripe:production": "node scripts/deploy-production.js --stripe-production",
    "stripe:test": "node scripts/deploy-production.js --stripe-test",
    "test:stripe:config": "node scripts/test-stripe-integration.js --check-config",
    "test:stripe:api": "node scripts/test-stripe-integration.js --test-api",
    "test:stripe:webhook": "node scripts/test-stripe-integration.js --test-webhook",
    "test:stripe:monitor": "node scripts/test-stripe-integration.js --monitor-payments",
    "test:stripe:full": "node scripts/test-stripe-integration.js --full-test",
    "verify:payments:recent": "node scripts/verify-payment-flow.js --recent",
    "verify:payments:audit": "node scripts/verify-payment-flow.js --full-audit",
    "verify:payment": "node scripts/verify-payment-flow.js --payment-id",
    "verify:session": "node scripts/verify-payment-flow.js --session-id"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.53.0",
    "@vercel/analytics": "^1.1.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.536.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
