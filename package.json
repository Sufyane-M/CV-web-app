{
  "name": "resumeai-smart-cv-analysis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && node -e \"console.log('Open dist/stats.html generated by visualizer if needed')\"",
    "build:optimized": "node scripts/build-optimized.js",
    "preview": "vite preview",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --collect.numberOfRuns=3 --assert",
    "perf:monitor": "node -e \"console.log('Performance monitoring active')\"",
    "perf:budget": "node scripts/performance-budget.js",
    "perf:check": "npm run build:optimized && npm run perf:budget",
    "test:perf": "npm run perf:check",
    "check": "npm run build && npm run perf:budget",
    "monitor:start": "npm run dev & npm run perf:monitor",
    "ci:perf": "npm run build:optimized && npm run lighthouse:ci && npm run perf:budget"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.536.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-compression": "^0.5.1"
  }
}
